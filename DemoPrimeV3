public class DemoPrimeV3{
    public static void main(String args[]) {
        int i,num,flag=0;
        num=Integer.parseInt(args[0]);
        for(i=2;i<=num/2;i++)
        {
            if(num%i==0)
            {
                System.out.println("It is not a prime number");
                flag=1;
                break;
            }
        }
        if(flag==0)
        {
            System.out.println("It is a prime number");
        }
    }
}
